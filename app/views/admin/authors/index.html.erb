<% Author.all.each do |x| %>
  <div class='author'>
    <a href='<%= url_for edit_admin_author_url(x.id) %>'><button class='admin-button'>Edit</button></a>
    <%= button_to "X", admin_author_url(x.id), method: :delete %>
  <h2>
   <a href='<%= author_url(x.id)%>'> <%= x.name %>
  </h2></a>
</div>
<% end %>
